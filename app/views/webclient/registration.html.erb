<h1> User Registrieren</h1>


<%= render 'form' %>

<%= form_for(:webclient, as: :webclient, url: { action: 'register' }) do |f| %>
    <div class="form-group"><%= f.text_field        :username,             :placeholder => 'Name',:autofocus => true   ,:class=>'form-control'  %> </div>
    <div class="form-group"><%= f.text_field        :password,             :placeholder => 'Password'                  ,:class=>'form-control'   %> </div>
    <%= f.submit "Registrieren", :class=>'btn btn-primary block full-width m-b' %>
<%end%>